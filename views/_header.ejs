<style>
    header {
      background-color: #f2f2f2;
      padding: 10px;
      display: flex;
      justify-content: space-between;
      align-items: center;
    }
  
    header h1 {
      margin: 0;
      font-size: 24px;
    }
  
    header ul {
      list-style-type: none;
      margin: 0;
      padding: 0;
      display: flex;
      align-items: center;
    }
  
    header li {
      margin-right: 10px;
    }
  
    header a {
      text-decoration: none;
      color: #333;
    }
  </style>
  
  <header>
    <h1>Divish-Tech</h1>
  
    <ul>
      <% if (locals.user) { %>
      <li>
        <a href="/users/profile">
          <%= user.name %>
        </a>
      </li>
      <li>
        <a href="/users/dashboard">Dashboard</a>
      </li>
      <li>
        <a href="/users/signinurl">signinurl</a>
      </li>
      <li>
        <a href="/users/sign-out">Sign Out</a>
      </li>
      <% } else { %>
      <li>
        <a href="/users/sign-in">Sign In</a>
      </li>
      <li>
        <a href="/users/sign-up">Sign Up</a>
      </li>
      <% } %>
    </ul>
  </header>
  